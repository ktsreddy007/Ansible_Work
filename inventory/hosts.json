{
    "all": {
        "children": {
            "control": {
                "hosts": {
                    "ubuntu-c": {
                        "ansible_connection": "local"
                    }
                }
            },
            "centos": {
                "hosts": {
                    "centos1": {
                        "ansible_ssh_port": 2222,
                        "ansible_user": "root"
                    },
                    "centos2": {
                        "ansible_user": "root",
                        "ansible_ssh_port": 22
                    },
                    "centos3": {
                        "ansible_user": "root",
                        "ansible_ssh_port": 22
                    }
                }
            },
            "ubuntu": {
                "hosts": {
                    "ubuntu1": {
                        "ansible_become": true,
                        "ansible_become_pass": "password"
                    },
                    "ubuntu2": {
                        "ansible_become": true,
                        "ansible_become_pass": "password"
                    },
                    "ubuntu3": {
                        "ansible_become": true,
                        "ansible_become_pass": "password"
                    }
                },
                "vars": {
                    "ansible_become": true,
                    "ansible_become_pass": "password"
                }
            },
            "linux": {
                "children": {
                    "centos": {},
                    "ubuntu": {}
                }
            }
        }
    }
}
